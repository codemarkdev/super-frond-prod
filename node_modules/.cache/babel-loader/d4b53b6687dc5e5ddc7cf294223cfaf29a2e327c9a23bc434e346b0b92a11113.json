{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nexport default {\n  name: 'VisorPDF',\n  props: {\n    titulo: {\n      type: String,\n      default: 'Comprobante de Venta'\n    },\n    urlBase: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      mostrar: false,\n      pdfUrl: null,\n      cargando: false,\n      pdfCargado: false,\n      error: false,\n      mensajeError: '',\n      imprimiendo: false\n    };\n  },\n  methods: {\n    abrir(idDocumento) {\n      this.resetEstados();\n      this.cargando = true;\n      this.cleanupPdfUrl();\n      this.pdfUrl = `${this.urlBase}/${idDocumento}?t=${Date.now()}`;\n      this.mostrar = true;\n    },\n    onCargado() {\n      this.cargando = false;\n      this.pdfCargado = true;\n    },\n    onError() {\n      this.cargando = false;\n      this.error = true;\n      this.mensajeError = 'Error al cargar el documento';\n    },\n    cerrar() {\n      this.mostrar = false;\n      this.cleanupPdfUrl();\n    },\n    async imprimir() {\n      if (!this.pdfCargado) return;\n      this.imprimiendo = true;\n      try {\n        const iframe = this.$refs.pdfIframe;\n        iframe.contentWindow.focus();\n        iframe.contentWindow.print();\n        setTimeout(this.cerrar, 1000);\n      } catch {\n        this.imprimirFallback();\n      } finally {\n        this.imprimiendo = false;\n      }\n    },\n    imprimirFallback() {\n      const printWindow = window.open(this.pdfUrl, '_blank');\n      printWindow.onload = () => printWindow.print();\n      setTimeout(this.cerrar, 1000);\n    },\n    cleanupPdfUrl() {\n      if (this.pdfUrl) {\n        URL.revokeObjectURL(this.pdfUrl);\n        this.pdfUrl = null;\n      }\n    },\n    resetEstados() {\n      this.pdfCargado = this.error = false;\n      this.mensajeError = '';\n    }\n  },\n  beforeDestroy() {\n    this.cleanupPdfUrl();\n  }\n};","map":{"version":3,"names":["name","props","titulo","type","String","default","urlBase","required","data","mostrar","pdfUrl","cargando","pdfCargado","error","mensajeError","imprimiendo","methods","abrir","idDocumento","resetEstados","cleanupPdfUrl","Date","now","onCargado","onError","cerrar","imprimir","iframe","$refs","pdfIframe","contentWindow","focus","print","setTimeout","imprimirFallback","printWindow","window","open","onload","URL","revokeObjectURL","beforeDestroy"],"sources":["src/components/Ventas/VisorPDF.vue"],"sourcesContent":["<template>\r\n  <b-modal :active.sync=\"mostrar\" :width=\"800\" scroll=\"keep\" aria-role=\"dialog\">\r\n    <div class=\"modal-card\">\r\n      <header class=\"modal-card-head\">\r\n        <p class=\"modal-card-title\">{{ titulo }}</p>\r\n        <button class=\"delete\" @click=\"cerrar\"></button>\r\n      </header>\r\n      \r\n      <section class=\"modal-card-body\">\r\n        <div v-if=\"cargando\" class=\"loading-container\">\r\n          <b-loading :active=\"true\"></b-loading>\r\n          <p>Cargando documento...</p>\r\n        </div>\r\n        \r\n        <div v-if=\"error\" class=\"error-container\">\r\n          <div class=\"notification is-danger\">\r\n            {{ mensajeError }}\r\n          </div>\r\n        </div>\r\n        \r\n        <iframe\r\n          v-show=\"pdfCargado\"\r\n          :src=\"pdfUrl\"\r\n          ref=\"pdfIframe\"\r\n          @load=\"onCargado\"\r\n          @error=\"onError\"\r\n        ></iframe>\r\n      </section>\r\n      \r\n      <footer class=\"modal-card-foot\">\r\n        <button class=\"button\" @click=\"cerrar\">Cerrar</button>\r\n        <button class=\"button is-primary\" \r\n                @click=\"imprimir\"\r\n                :disabled=\"!pdfCargado || imprimiendo\">\r\n          <i class=\"mdi mdi-printer\"></i>\r\n          <span>{{ imprimiendo ? 'Imprimiendo...' : 'Imprimir' }}</span>\r\n        </button>\r\n      </footer>\r\n    </div>\r\n  </b-modal>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'VisorPDF',\r\n  props: {\r\n    titulo: { type: String, default: 'Comprobante de Venta' },\r\n    urlBase: { type: String, required: true }\r\n  },\r\n  data() {\r\n    return {\r\n      mostrar: false,\r\n      pdfUrl: null,\r\n      cargando: false,\r\n      pdfCargado: false,\r\n      error: false,\r\n      mensajeError: '',\r\n      imprimiendo: false\r\n    }\r\n  },\r\n  methods: {\r\n    abrir(idDocumento) {\r\n      this.resetEstados();\r\n      this.cargando = true;\r\n      this.cleanupPdfUrl();\r\n      this.pdfUrl = `${this.urlBase}/${idDocumento}?t=${Date.now()}`;\r\n      this.mostrar = true;\r\n    },\r\n    \r\n    onCargado() {\r\n      this.cargando = false;\r\n      this.pdfCargado = true;\r\n    },\r\n    \r\n    onError() {\r\n      this.cargando = false;\r\n      this.error = true;\r\n      this.mensajeError = 'Error al cargar el documento';\r\n    },\r\n    \r\n    cerrar() {\r\n      this.mostrar = false;\r\n      this.cleanupPdfUrl();\r\n    },\r\n    \r\n    async imprimir() {\r\n      if (!this.pdfCargado) return;\r\n      \r\n      this.imprimiendo = true;\r\n      try {\r\n        const iframe = this.$refs.pdfIframe;\r\n        iframe.contentWindow.focus();\r\n        iframe.contentWindow.print();\r\n        setTimeout(this.cerrar, 1000);\r\n      } catch {\r\n        this.imprimirFallback();\r\n      } finally {\r\n        this.imprimiendo = false;\r\n      }\r\n    },\r\n    \r\n    imprimirFallback() {\r\n      const printWindow = window.open(this.pdfUrl, '_blank');\r\n      printWindow.onload = () => printWindow.print();\r\n      setTimeout(this.cerrar, 1000);\r\n    },\r\n    \r\n    cleanupPdfUrl() {\r\n      if (this.pdfUrl) {\r\n        URL.revokeObjectURL(this.pdfUrl);\r\n        this.pdfUrl = null;\r\n      }\r\n    },\r\n    \r\n    resetEstados() {\r\n      this.pdfCargado = this.error = false;\r\n      this.mensajeError = '';\r\n    }\r\n  },\r\n  beforeDestroy() {\r\n    this.cleanupPdfUrl();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.modal-card {\r\n  width: 90%;\r\n  max-width: 800px;\r\n  height: 90vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.modal-card-body {\r\n  flex: 1;\r\n  position: relative;\r\n}\r\n\r\n.loading-container,\r\n.error-container {\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\niframe {\r\n  width: 100%;\r\n  height: 100%;\r\n  min-height: 500px;\r\n  border: none;\r\n}\r\n\r\n@media print {\r\n  body * { visibility: hidden; }\r\n  .modal-card, .modal-card * { visibility: visible; }\r\n  .modal-card {\r\n    position: absolute;\r\n    inset: 0;\r\n    margin: 0;\r\n  }\r\n  .modal-card-foot { display: none; }\r\n}\r\n</style>\r\n\r\n"],"mappings":";;;AA2CA;EACAA,IAAA;EACAC,KAAA;IACAC,MAAA;MAAAC,IAAA,EAAAC,MAAA;MAAAC,OAAA;IAAA;IACAC,OAAA;MAAAH,IAAA,EAAAC,MAAA;MAAAG,QAAA;IAAA;EACA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,MAAA;MACAC,QAAA;MACAC,UAAA;MACAC,KAAA;MACAC,YAAA;MACAC,WAAA;IACA;EACA;EACAC,OAAA;IACAC,MAAAC,WAAA;MACA,KAAAC,YAAA;MACA,KAAAR,QAAA;MACA,KAAAS,aAAA;MACA,KAAAV,MAAA,WAAAJ,OAAA,IAAAY,WAAA,MAAAG,IAAA,CAAAC,GAAA;MACA,KAAAb,OAAA;IACA;IAEAc,UAAA;MACA,KAAAZ,QAAA;MACA,KAAAC,UAAA;IACA;IAEAY,QAAA;MACA,KAAAb,QAAA;MACA,KAAAE,KAAA;MACA,KAAAC,YAAA;IACA;IAEAW,OAAA;MACA,KAAAhB,OAAA;MACA,KAAAW,aAAA;IACA;IAEA,MAAAM,SAAA;MACA,UAAAd,UAAA;MAEA,KAAAG,WAAA;MACA;QACA,MAAAY,MAAA,QAAAC,KAAA,CAAAC,SAAA;QACAF,MAAA,CAAAG,aAAA,CAAAC,KAAA;QACAJ,MAAA,CAAAG,aAAA,CAAAE,KAAA;QACAC,UAAA,MAAAR,MAAA;MACA;QACA,KAAAS,gBAAA;MACA;QACA,KAAAnB,WAAA;MACA;IACA;IAEAmB,iBAAA;MACA,MAAAC,WAAA,GAAAC,MAAA,CAAAC,IAAA,MAAA3B,MAAA;MACAyB,WAAA,CAAAG,MAAA,SAAAH,WAAA,CAAAH,KAAA;MACAC,UAAA,MAAAR,MAAA;IACA;IAEAL,cAAA;MACA,SAAAV,MAAA;QACA6B,GAAA,CAAAC,eAAA,MAAA9B,MAAA;QACA,KAAAA,MAAA;MACA;IACA;IAEAS,aAAA;MACA,KAAAP,UAAA,QAAAC,KAAA;MACA,KAAAC,YAAA;IACA;EACA;EACA2B,cAAA;IACA,KAAArB,aAAA;EACA;AACA","ignoreList"